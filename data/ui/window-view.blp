using Gtk 4.0;
using Adw 1;

template $StormWindowView: Adw.ApplicationWindow {
    default-width: 600;
    default-height: 500;
    width-request: 360;
    height-request: 294;

    Adw.NavigationView navigation-view {
        $StormMainMenuView main-menu-view {
            service: template;
        }
    }
 }
